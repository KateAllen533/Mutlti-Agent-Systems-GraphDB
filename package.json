{
  "name": "multiagent-data-system",
  "version": "1.0.0",
  "description": "Multi-agent system for data loading, structuring, and graph modeling",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "electron .",
    "dev": "concurrently \"npm run server\" \"npm run electron-dev\"",
    "server": "node src/index.js",
    "electron-dev": "electron . --dev",
    "build": "npm run build-react && electron-builder",
    "build-react": "webpack --mode production",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "lint": "pnpm exec eslint src/",
    "typecheck": "pnpm exec tsc --noEmit",
    "test": "pnpm exec jest"
  },
  "main": "electron/main.js",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "multer": "^1.4.5-lts.1",
    "csv-parser": "^3.0.0",
    "xlsx": "^0.18.5",
    "neo4j-driver": "^5.15.0",
    "sqlite3": "^5.1.6",
    "mysql2": "^3.6.5",
    "pg": "^8.11.3",
    "openai": "^4.19.0",
    "@anthropic-ai/sdk": "^0.61.0",
    "dotenv": "^16.3.1",
    "ws": "^8.14.2",
    "uuid": "^9.0.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "joi": "^17.11.0",
    "winston": "^3.11.0",
    "axios": "^1.6.2",
    "socket.io": "^4.7.4",
    "vis-network": "^9.1.6",
    "electron": "^28.0.0",
    "electron-builder": "^24.6.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.0",
    "antd": "^5.12.0",
    "@ant-design/icons": "^5.2.0",
    "d3": "^7.8.5",
    "recharts": "^2.8.0",
    "cytoscape": "^3.26.0",
    "cytoscape-dagre": "^2.5.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/multer": "^1.4.11",
    "@types/uuid": "^9.0.7",
    "@types/lodash": "^4.14.202",
    "@types/d3": "^7.4.3",
    "@types/ws": "^8.5.10",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "typescript": "^5.3.2",
    "nodemon": "^3.0.2",
    "eslint": "^8.55.0",
    "prettier": "^3.1.0",
    "jest": "^29.7.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "html-webpack-plugin": "^5.5.3",
    "css-loader": "^6.8.1",
    "style-loader": "^3.3.3",
    "babel-loader": "^9.1.3",
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@babel/preset-react": "^7.22.0",
    "concurrently": "^8.2.2",
    "node-fetch": "^3.3.2",
    "form-data": "^4.0.0"
  },
  "build": {
    "appId": "com.multiagent.data-system",
    "productName": "Multi-Agent Data System",
    "directories": {
      "output": "dist"
    },
    "files": [
      "electron/**/*",
      "src/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "win": {
      "target": "nsis",
      "icon": "assets/icon.ico"
    },
    "mac": {
      "target": "dmg",
      "icon": "assets/icon.icns"
    },
    "linux": {
      "target": "AppImage",
      "icon": "assets/icon.png"
    }
  },
  "keywords": [
    "multiagent",
    "data-processing",
    "neo4j",
    "graph-database",
    "llm",
    "dashboard"
  ],
  "author": "Multi-Agent Data System",
  "license": "MIT"
}
